{
  "name": "us proxy",
  "version":"1.8",
  "description": "us proxy",
  "manifest_version": 3,
  "action": {
    "default_icon": {
        "16": "images/16.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "default_title": "us proxy",
    "default_popup": "index.html"
  },
  "options_page": "index.html",
  "background": {
    "service_worker": "static/js/background.js",
    "type": "module"
  },
  "host_permissions":[
    "*://*/*"
  ],
  "declarative_net_request":{
    "rule_resources":[{
      "id": "rule_1",
      "enabled": true,
      "path": "rule_1.json"
    }]
  },
  "permissions": [
    "cookies",
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "background"
  ]
}
