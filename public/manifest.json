{
  "name": "wali proxy",
  "version":"1.0",
  "description": "wali proxy",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
        "16": "images/16.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "default_title": "wali proxy",
    "default_popup": "index.html"
  },
  "options_page": "index.html",
  "background": {
    "scripts": ["static/js/background.js"],
    "persistent": true
  },
  "permissions": [
    "storage",
    "webRequestBlocking",
    "webRequest",
    "background",
    "<all_urls>"
  ]
}
