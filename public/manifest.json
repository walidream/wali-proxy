{
  "name": "wali proxy",
  "version":"1.2",
  "description": "wali proxy",
  "manifest_version": 3,
  "action": {
    "default_icon": {
        "16": "images/16.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "default_title": "wali proxy",
    "default_popup": "index.html"
  },
  "options_page": "index.html",
  "background": {
    "service_worker": "static/js/background.js",
    "type": "module"
  },
  "host_permissions":[
    "*://*/*"
  ],
  "permissions": [
    "cookies",
    "storage",
    "webRequest",
    "background"
  ]
}
